{
  "name": "vmto",
  "version": "1.1.1",
  "description": "velocity(vm) to any other templates.",
  "main": "index.js",
  "scripts": {
    "test": "mocha -R spec -t 5000 test/*.test.js",
    "test-cov": "node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -t 5000 test/*.test.js",
    "test-travis": "node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- -t 5000 test/*.test.js",
    "jshint": "jshint .",
    "autod": "autod -w --prefix '~' && npm run cnpm",
    "cnpm": "npm install --registry=https://registry.npm.taobao.org",
    "contributors": "contributors -f plain -o AUTHORS"
  },
  "config": {
    "cov": {
      "threshold": 95
    }
  },
  "dependencies": {
    "velocity": "~0.7.0"
  },
  "devDependencies": {
    "autod": "*",
    "contributors": "*",
    "cov": "*",
    "istanbul-harmony": "*",
    "jshint": "*",
    "mocha": "*",
    "should": "~4.1.0"
  },
  "homepage": "https://github.com/node-modules/vmto",
  "repository": {
    "type": "git",
    "url": "git://github.com/node-modules/vmto.git",
    "web": "https://github.com/node-modules/vmto"
  },
  "bugs": {
    "url": "https://github.com/node-modules/vmto/issues",
    "email": "fengmk2@gmail.com"
  },
  "keywords": [
    "vmto", "velocity", "vm",
    "nunjucks"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "fengmk2 <fengmk2@gmail.com> (http://fengmk2.github.com)",
  "license": "MIT"
}
